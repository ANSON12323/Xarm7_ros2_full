            </geometry>   
            <material name="grey" />
        </collision>
    </link>

    <joint name="joint5" type="revolute">
        <parent link="link4"/>
        <child link="link5"/>
        <origin xyz="0.0775 -0.3425 0" rpy="1.5708 0 0"/>
        <axis xyz="0 0 1"/>
        <limit lower="-6.283" upper="6.283" effort="50" velocity="3.142"/>
