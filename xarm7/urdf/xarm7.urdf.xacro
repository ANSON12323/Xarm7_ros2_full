<?xml version="1.0"?>
<robot name="xarm7" xmlns:xacro="http://www.ros.org/wiki/xacro">

    <!-- Include Gazebo Parameters, ros2_control Plugins , Material -->
    <xacro:include filename="$(find xarm7)/urdf/xarm7_gazebo.xacro" />
    <xacro:include filename="$(find xarm7)/urdf/xarm7.ros2_control.xacro" />
    <xacro:include filename="$(find xarm7)/urdf/common_properties.xacro" />
    
    <!-- Links -->
    <link name="world"/>

    <link name="base_link">
        <xacro:default_inertial mass="5"/>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://xarm7/meshes/link_base.stl" scale="1.0 1.0 1.0"/>
            </geometry>   
            <material name="blue" />
        </visual>
        <gazebo reference="base_link">
            <material>Gazebo/Red</material> <!-- For Gazebo -->
            <static>true</static>
        </gazebo>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://xarm7/meshes/link_base.stl" scale="1.0 1.0 1.0"/>
            </geometry>   
            <material name="blue" />
        </collision>
    </link>

    <joint name="virtual_joint" type="fixed">
        <parent link="world"/>
        <child link="base_link"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint> 
    
    <link name="link1">
        <xacro:default_inertial mass="1.5"/>
        <visual>
            <origin rpy="0 0 0" xyz="-0 0 0"/>
            <geometry>
                <mesh filename="package://xarm7/meshes/link1.stl" scale="1.0 1.0 1.0"/>
            </geometry>   
            <material name="grey" />
        </visual>
        <gazebo reference="link1">
            <material>Gazebo/Red</material> <!-- For Gazebo -->
        </gazebo>
        <collision>
            <origin rpy="0 0 0" xyz="-0 0 0"/>
            <geometry>
                <mesh filename="package://xarm7/meshes/link1.stl" scale="1.0 1.0 1.0"/>
            </geometry>   
            <material name="grey" />
        </collision>
    </link>

    <joint name="joint1" type="revolute">
        <parent link="base_link"/>
        <child link="link1"/>
        <origin xyz="0 0 0.267" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
        <limit lower="-6.283" upper="6.283" effort="50" velocity="3.142"/>
    </joint>

    <link name="link2">
        <xacro:default_inertial mass="1.5"/>
        <visual>
            <origin rpy="0 0 0" xyz="-0 0 0"/>
            <geometry>
                <mesh filename="package://xarm7/meshes/link2.stl" scale="1.0 1.0 1.0"/>
            </geometry>
            <material name="blue" />
        </visual>
        <gazebo reference="link2">
            <material>Gazebo/Red</material> <!-- For Gazebo -->
        </gazebo>
        <collision>
            <origin rpy="0 0 0" xyz="-0 0 0"/>
            <geometry>
                <mesh filename="package://xarm7/meshes/link2.stl" scale="1.0 1.0 1.0"/>
            </geometry>   
            <material name="grey" />
        </collision>
    </link>

    <joint name="joint2" type="revolute">
        <parent link="link1"/>
        <child link="link2"/>
        <origin xyz="0 0 0" rpy="-1.5708 0.0 0.0"/>
        <axis xyz="0 0 1"/>
        <limit lower="-2.059" upper="2.094" effort="50" velocity="3.142"/>
    </joint>

    <link name="link3">
        <xacro:default_inertial mass="1.2"/>
        <visual>
            <origin rpy="0 0 0" xyz="-0 0 0"/>
            <geometry>
