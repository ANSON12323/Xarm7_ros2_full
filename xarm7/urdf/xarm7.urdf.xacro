            <geometry>
                <mesh filename="package://xarm7/meshes/link4.stl" scale="1.0 1.0 1.0"/>
            </geometry>   
            <material name="grey" />
        </collision>
    </link>

    <joint name="joint4" type="revolute">
        <parent link="link3"/>
        <child link="link4"/>
        <origin xyz="0.0525 0 0" rpy="1.5708 0 0"/>
        <axis xyz="0 0 1"/>
        <limit lower="-0.192" upper="3.927" effort="50" velocity="3.142"/>
    </joint>

    <link name="link5">
        <xacro:default_inertial mass="1.1"/>
        <visual>
            <origin rpy="0 0 0" xyz="-0 0 0"/>
            <geometry>
