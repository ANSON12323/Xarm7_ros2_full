            <geometry>
                <mesh filename="package://xarm7/meshes/link3.stl" scale="1.0 1.0 1.0"/>
            </geometry>   
            <material name="grey" />
        </collision>
    </link>

    <joint name="joint3" type="revolute">
        <parent link="link2"/>
        <child link="link3"/>
        <origin xyz="0 -0.293 0" rpy="1.5708 0 0"/>
        <axis xyz="0 0 1"/>
        <limit lower="-6.283" upper="6.283" effort="50" velocity="3.142"/>
    </joint>

    <link name="link4">
