<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="xarm7">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import xarm7 urdf file -->
    <xacro:include filename="$(find xarm7)/urdf/xarm7.urdf.xacro" />

    <!-- Import control_xacro -->
    <xacro:include filename="xarm7.ros2_control.xacro" />


    <xacro:xarm7_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
